{
	"name": "laravel/laravel",
	"description": "The Laravel Framework.",
	"keywords": ["framework", "laravel"],
	"license": "MIT",
	"require": {
		"laravel/framework": "4.2.*",
		"artdarek/oauth-4-laravel": "dev-master",
		"intervention/image": "dev-master",
		"philf/setting": "dev-master",
		"cartalyst/sentry": "2.1.*",
		"jenssegers/agent": "*",
		"olliefordandco/housekeeper": "dev-master"	
	},
	"repositories": [
		{
			"type": "vcs",
			"url": "https://github.com/OllieFordandCo/housekeeper"
		}
	],	
	"autoload": {
		"classmap": [
			"app/commands",
			"app/controllers",
			"app/models",
			"app/database/migrations",
			"app/database/seeds",
			"app/tests/TestCase.php"
		]
	},	
	"scripts": {
		"post-install-cmd": [
			"/usr/local/php5429-cgi/bin/php artisan clear-compiled",
			"/usr/local/php5429-cgi/bin/php artisan optimize"
		],
		"post-update-cmd": [
			"/usr/local/php5429-cgi/bin/php artisan clear-compiled",
			"/usr/local/php5429-cgi/bin/php artisan optimize"
		],
		"post-create-project-cmd": [
			"/usr/local/php5429-cgi/bin/php artisan key:generate"
		]
	},
	"config": {
		"preferred-install": "dist"
	},
	"minimum-stability": "dev"
}
